{
  "name": "spice_rack_backend",
  "version": "1.0.0",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "start": "npx tsx src/server.ts",
    "dev": "npx tsx src/server.ts",
    "prepare": "husky",
    "db:generate": "dotenv -f .env.development -- prisma generate --schema src/prisma/schema.prisma",
    "db:migrate": "dotenv -f .env.development -- prisma migrate dev --schema src/prisma/schema.prisma",
    "db:studio": "dotenv -f .env.development -- prisma studio --schema src/prisma/schema.prisma",
    "db:reset": "pnpm db:gen && dotenv -f .env.development -- prisma migrate reset --schema src/prisma/schema.prisma",
    "db:seed": "dotenv -f .env.development -- prisma db seed"
  },
  "prisma": {
    "seed": "dotenv -f .env.development -- tsx src/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.713.0",
    "@prisma/client": "^6.1.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ensign": "^1.3.0",
    "envalid": "^8.0.0",
    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "maskdata": "^1.3.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
